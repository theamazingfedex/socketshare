{
  "name": "socketshare",
  "version": "1.2.2",
  "description": "Create a temporary fileserver to share files over the internet.",
  "scripts": {
    "test": "babel-tape-runner 'tests/**/*.js'",
    "start": "npm run build && node bin/app",
    "build": "babel src --out-dir bin",
    "publish": "babel src --out-dir bin && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theamazingfedex/socketshare"
  },
  "author": "Daniel Wood",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theamazingfedex/socketshare"
  },
  "homepage": "https://github.com/theamazingfedex/socketshare#README",
  "bin": {
    "socketshare": "bin/app.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "content-disposition": "0.5.0",
    "copy-paste": "1.1.3",
    "express": "4.13.x",
    "externalip": "1.0.1",
    "minimist": "1.1.3",
    "tinyurl": "1.0.8"
  },
  "devDependencies": {
    "babel-cli": "6.4.x",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.x",
    "babel-preset-es2015": "6.3.x",
    "babel-tape-runner": "2.0.0",
    "blue-tape": "0.1.x",
    "faucet": "0.0.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "socketshare",
    "share",
    "files",
    "local",
    "server",
    "external",
    "cli",
    "terminal",
    "cmd",
    "node"
  ]
}
